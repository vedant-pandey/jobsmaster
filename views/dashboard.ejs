<% include partials/header %>

    <div style="padding-top: 170px;">
        <h1 class="mt-4 adti">Admin Panel</h1>
        <p class="lead mb-3 adtis">Welcome <%= user.name %></p>
        <ul>
            <% informations.forEach(function(info){ %>
                <li>
                    <div class='adid'>
                        <h2>Name - <span class='adis'><%= info.name %></span></h2>
                        <p>Email - <span class='adis'><%= info.email %></span></p>
                        <p>Phone - <span class='adis'><%= info.phone %></span></p>
                        <p>City - <span class='adis'><%= info.city %></span></p>
                        <p><a href="/dashboard/<%= info._id %>" class='adis'>Open Resume</a></p>
                    </div>
                </li>
                <hr style="border-top: 1px solid #343a4085;">
                <br>
            <% }); %>
        </ul>
        <a href="/admin/logout" class="btn btn-secondary"style="width: 100%;height: 50px;margin-bottom: 40px;">Logout</a>
    </div>

<% include partials/footer %>